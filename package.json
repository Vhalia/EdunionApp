{
  "name": "EdunionApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start:local:win": "SET ENVFILE=.env.local && react-native start",
    "start:local": "ENVFILE=.env.local && react-native start",
    "android:local:win": "SET ENVFILE=.env.local && react-native run-android",
    "android:local": "ENVFILE=.env.local && react-native run-android",
    "ios:local:win": "SET ENVFILE=.env.local && react-native run-ios",
    "ios:local": "ENVFILE=.env.local && react-native run-ios",
    "start:staging:win": "SET ENVFILE=.env.staging && react-native start",
    "start:staging": "ENVFILE=.env.staging && react-native start",
    "android:staging:win": "SET ENVFILE=.env.staging && react-native run-android",
    "android:staging": "ENVFILE=.env.staging && react-native run-android",
    "ios:staging:win": "SET ENVFILE=.env.staging && react-native run-ios",
    "ios:staging": "ENVFILE=.env.staging && react-native run-ios",
    "android:release:win": "SET ENVFILE=.env && cd android && ./gradlew bundleRelease --no-daemon",
    "android:release": "ENVFILE=.env && chmod 777 android/gradlew && cd android && ./gradlew bundleRelease --no-daemon",
    "android:release:staging:win": "SET ENVFILE=.env.staging && cd android && ./gradlew bundleRelease --no-daemon",
    "android:release:staging": "ENVFILE=.env.staging && chmod 777 android/gradlew && cd android && ./gradlew bundleRelease --no-daemon",
    "ios:clean": "xattr -w com.apple.xcode.CreatedByBuildSystem true ./ios/build && cd ios && rm -rf ./build && xcodebuild clean && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && pod cache clean --all && pod deintegrate && pod setup && pod install && pod update && cd ..",
    "lint": "eslint .",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/datetimepicker": "^8.0.0",
    "@react-native-firebase/app": "^20.4.0",
    "@react-native-firebase/app-distribution": "^20.4.0",
    "@react-native-firebase/messaging": "^20.4.0",
    "@react-native-picker/picker": "^2.7.7",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.9.18",
    "@shopify/flash-list": "^1.6.4",
    "patch-package": "^8.0.0",
    "react": "18.3.1",
    "react-native": "^0.76.0",
    "react-native-config": "^1.5.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-picker-select": "^9.1.3",
    "react-native-reanimated": "^3.16.1",
    "react-native-reanimated-carousel": "v4.0.0-canary.22",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.8.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-toast-message": "2.2.0",
    "react-native-url-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^15.0.0",
    "@react-native-community/cli-platform-android": "^15.0.0",
    "@react-native-community/cli-platform-ios": "^15.0.0",
    "@react-native/babel-preset": "^0.76.0",
    "@react-native/eslint-config": "0.76.1",
    "@react-native/metro-config": "^0.76.0",
    "@react-native/typescript-config": "0.76.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
